	<Style x:Key="Basic_Button" TargetType="{x:Type Button}">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type Button}">
					<Grid>
						<Rectangle x:Name="Border_1" Stroke="Black" Fill="Black"/>
						<Rectangle x:Name="Border_2" Margin="3" Opacity="0.85">
							<Rectangle.Fill>
								<LinearGradientBrush EndPoint="-0.053,-0.019" MappingMode="RelativeToBoundingBox" StartPoint="0.756,0.88">
									<GradientStop Color="#FF252323" Offset="0.217"/>
									<GradientStop Color="White" Offset="0.775"/>
								</LinearGradientBrush>
							</Rectangle.Fill>
						</Rectangle>
						<Image Margin="0,0,3,3" Source="Play.png" Stretch="Fill" RenderTransformOrigin="0.031,0.531"/>
						<ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Content=""/>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="IsFocused" Value="True"/>
						<Trigger Property="IsDefaulted" Value="True"/>
						<Trigger Property="IsMouseOver" Value="True"/>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Fill" TargetName="Border_1" Value="Black"/>
							<Setter Property="Fill" TargetName="Border_2">
								<Setter.Value>
									<LinearGradientBrush EndPoint="-0.053,-0.019" MappingMode="RelativeToBoundingBox" StartPoint="0.756,0.88">
										<GradientStop Color="White" Offset="0.217"/>
										<GradientStop Color="White" Offset="0.775"/>
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter Property="Opacity" TargetName="Border_2" Value="0.75"/>
						</Trigger>
						<Trigger Property="IsEnabled" Value="False"/>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
